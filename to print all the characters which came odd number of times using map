#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s="mahaeswarreddy";
    map<char,int>mp;
    for(int i=0;i<s.size();i++)
    {
        mp[s[i]]++;
    }
    for(auto j:mp)
    {
        if(j.second%2!=0)
        {
            cout<<j.first<<" "<<j.second<<endl;
        }
    }
}
