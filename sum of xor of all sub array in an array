//given an array a of size n.find the sum of xor of all sub array.


#include <iostream>
using namespace std;

int main()
{
    int sum=0;
   int arr[3]={1,2,3};
   for(int i=0;i<3;i++)
   {
       for(int j=i;j<3;j++)
       {
           int xorr=0;
           for(int k=i;k<=j;k++)
           {
               xorr=xorr^arr[k];
           }
           sum+=xorr;
       }
   }
   cout<<"sum of xor of subarray : "<<sum;
}
