#include<bits/stdc++.h>
using namespace std;
int main()
{
    string s="mahesh";
    string s1="amhseh";
    vector<int> v;
    vector<int>v1;
    for(int i=0;i<s.length();i++)
    {
        v.push_back((int)s[i]);
        v1.push_back((int)s1[i]);
    }
    sort(v.begin(),v.end());
    sort(v1.begin(),v1.end());
    if(v==v1) cout<<"yes";
    else cout<<"no";
}
